<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.football.statisticstracker.SignUpController">
   <children>
      <TextField fx:id="username" layoutX="300.0" layoutY="48.0" prefHeight="27.0" prefWidth="239.0" promptText="Username" />
      <PasswordField fx:id="password" layoutX="300.0" layoutY="98.0" prefHeight="28.0" prefWidth="239.0" promptText="Password" />
      <PasswordField fx:id="password1" layoutX="300.0" layoutY="143.0" prefHeight="28.0" prefWidth="239.0" promptText="Re-Enter Password" />
      <Button fx:id="signupbutton" layoutX="374.0" layoutY="234.0" mnemonicParsing="false" onAction="#sigunUp" text="SignUp" />
      <Button fx:id="backbutton" layoutX="382.0" layoutY="326.0" mnemonicParsing="false" onAction="#back" text="back" />
      <Label fx:id="statuslabel" layoutX="300.0" layoutY="200.0" prefHeight="20.0" prefWidth="239.0" />
      <Button fx:id="clearbutton" layoutX="381.0" layoutY="287.0" mnemonicParsing="false" onAction="#clear" text="clear" />
   </children>
